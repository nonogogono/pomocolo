<ul class="users follow">
  <% users.each do |user| %>
    <li>
      <%= link_to gravatar_for(user, size: 50), user_path(user) %>
        <div class="users-index-name">
        <%= link_to user.name, user_path(user) %>
        <%= user.profile %>
      </div>
    </li>
  <% end %>
</ul>
<%= paginate users %>
